<div class="story-element-image">
  <figure>
     <img src="{{focusedImageUrl(dataElement['image-s3-key'],config['image-cdn'], [16,9], dataElement['image-metadata'], {w: 900})}}"/>
  </figure>
  {% if dataElement['title'] %}
    <div class="story-element-image-text">
      {{dataElement['title'] | raw}}
    </div>
  {% endif %}
</div>
<div class="sourcefrom storysource">
  {{ dataElement["image-attribution"] | raw }}
</div>
