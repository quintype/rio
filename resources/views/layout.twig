    <div class="header">
      <div class="header__top-row">
        <div class="content">
        <div class="menubarimg mobile-menu js-menu">
          <ul class="menuitems">
            {% for menuItem in client.menuItems(nestedMenuItems) %}
            <li><a href="{{menuItem.url()}}">{{menuItem.title()}}</a></li>
            {% endfor %}
          </ul>
        </div>
        <div class="header-logo">
          <a href="/" title="demo" class="site-logo"></a>
        </div>
        <div class="desktop-menu">
          <ul class="desktop-menu__container">
            {% for menuItem in client.menuItems(nestedMenuItems) | slice(0,4) %}
            <li class="desktop-menu__item">
              <a href="/section{{menuItem.url()}}">{{menuItem.title()}}</a>
                {% if menuItem['children'] %}
                  <div class="desktop-submenu js-desktop-submenu">
                    <div class="desktop-submenu__wrapper">
                      <div class="desktop-submenu__content">
                        <ul class="desktop-submenu__container">
                          {% for submenu in client.menuItems(menuItem['children']) %}
                          <li class="desktop-submenu__item js-desktop-submenu__item"
                              data-section-id={{submenu['item-id']}}>
                            <a href="/section{{menuItem.url()}}{{submenu.url()}}">{{submenu.title()}}</a>
                          </li>
                          {% endfor %}
                        </ul>
                        <div class="submenu-stories-container js-submenu-stories-container"></div>
                      </div>
                    </div>
                  </div>
                {% endif %}
            </li>
            {% endfor %}
            <li class="desktop-menu__all-text js-menu">
              All
              <img src="{{ assetPath('icons/burger-menu.svg') }}">
              <div class="menuitems">
                <ul>
                  {% for menuItem in client.menuItems(nestedMenuItems) | slice(4) %}
                  <li class="menuitems__item"><a href="{{menuItem.url()}}">{{menuItem.title()}}</a></li>
                  {% endfor %}
                </ul>
              </div>
            </li>
          </ul>
        </div>

        <div class="mob_buttons">
          <ul>
            <li><span id="search_init1" class="search_butn">Search</span></li>
          </ul>
        </div>
        <div class="nav-search" >
          <div class="layout-search" id="main-search">
            <form class="search-form" role="search" action="/search">
              <div class="form-el">
                <input type="text" class="form-input" name="q" placeholder="Search for anything..">
              </div>
            </form>
          </div>
        </div>
        <div class="header-social-icon">
          <ul>
            <li class="socialicon"><a target="_blank" href="https://www.facebook.com/quintypeinc/"><img src="{{ assetPath('icons/facebook.svg') }}" /></a></li>
            <li class="socialicon"><a target="_blank" href="https://twitter.com/quintype_inc" ><img src="{{ assetPath('icons/twitter.svg') }}" /></a></li>
            <li class="socialicon"><a target="_blank" href="https://vimeo.com/user47797477"><img src="{{ assetPath('icons/vimeo.svg') }}" /></a></li>
            <li class="socialicon"><a target="_blank" href="https://www.linkedin.com/company/quintype-inc"><img src="{{ assetPath('icons/linkedin.svg') }}" /></a></li>
            <li><span id="search_init" class="search_butn">Search</span></li>
          </ul>
        </div>
      </div>
      </div>
      {% if breaking_news %}
      <div class="breaking-news">
        <div class="content">
          <div class="breaking-news__title">
            <span class="breaking-news__title__text">Breaking News</span>
          </div>
          <div class="breaking-news__headlines js-breakingnews-marquee">
            {% for breaking_story in breaking_news %}
            {% if breaking_story['metadata']['linked-story-slug'] %}
            <div class="breaking-news__headline">
              <a href="/{{breaking_story['metadata']['linked-story-slug']}}">{{breaking_story['headline'] | trim }}</a>
            </div>
            {% else %}
            <div class="breaking-news__headline">
              {{breaking_story['headline'] | trim }}
            </div>
            {% endif %}
            {% endfor %}
          </div>
        </div>
      </div>
      {% endif %}
